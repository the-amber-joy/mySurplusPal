{
    "manifest_version": 2,
    
    "name": "My Surplus Pal",
    "version": "1.0",
    "description": "Puts a positive spin on my occasional well-deserved bonus calories",

    "permissions": ["*://*.myfitnesspal.com/*"],
    "content_scripts": [{
          "matches": ["*://*.myfitnesspal.com/*"],
          "css": ["awesome.css"]
    }]
}
